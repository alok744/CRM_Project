@section Scripts
    {
    <script>

        //ADD validation.....

        function add_form() {
            //var c_id = $.trim($("#country_id_up").val());
            var state_name = $.trim($("#state_name").val());
            var country_id = $.trim($("#country_id").val());

            if (country_id == "") {
                alert("Select country name!");
                return false;
            }
            if (state_name == "") {
                alert("Enter state name!");
                return false;
            }
            $.ajax({
                type: "POST",
                url: '@Url.Action("add_m_state", "Master")',
                data: { state_name: state_name, country_id: country_id },
                // data: formData,
            }).done(function (res) {

                if (res.value == "invalid_id")
                    alert('Unable to add detail!');

                if (res.value == "True") {
                    $('#showModal').modal('hide');
                    Swal.fire({
                        text: "Data Inserted Successfully",
                        icon: "success",
                        confirmButtonClass: "btn btn-primary w-xs mt-2",
                        buttonsStyling: !1,

                    }).then((res) => {
                        // Code to run when the user clicks the "OK" button
                        if (res.isConfirmed) {
                            // Redirect the page
                            window.location.href = '@Url.Action("m_state_list", "Master")';
                        }
                    });


                }

            });

        }

        // Update Validation.....


        function check_form_update() {
            var st_id = $.trim($("#state_id_up").val());
            var country_name = $.trim($("#country_id_up").val());
            var state_name = $.trim($("#state_name_up").val());

            if (country_name == "") {
                alert("Select country name!")
                return false;
            }
            if (state_name == "") {
                alert("Enter state name!");
                return false;
            }
            $.ajax({
                type: "POST",
                url: '@Url.Action("edit_m_state", "Master")',
                data: { state_id: st_id, country_id: country_name, state_name: state_name },
                // data: formData,
            }).done(function (res) {

                if (res.value == "invalid_id")
                    alert('Unable to add detail!');

                if (res.value == "True") {
                    $('#updateModal').modal('hide');
                    Swal.fire({
                        text: "Data Updated Successfully",
                        icon: "success",
                        confirmButtonClass: "btn btn-primary w-xs mt-2",
                        buttonsStyling: !1,

                    }).then((res) => {
                        // Code to run when the user clicks the "OK" button
                        if (res.isConfirmed) {
                            // Redirect the page
                            window.location.href = '@Url.Action("m_state_list", "Master")';
                        }
                    });


                }

            });

        }

        // creating the function for delete button.....

        function deleted_data() {

            var state_id = $.trim($("#state_id").val());

            $.ajax({
                type: "POST",
                url: '@Url.Action("delete_m_state", "Master")',
                data: { state_id: state_id },
                // data: formData,
            }).done(function (res) {

                if (res.value == "invalid_id")
                    alert('Unable to add detail!');

                if (res.value == "True") {
                    $('#deleteRecordModal').modal('hide');
                    Swal.fire({
                        text: "Data Deleted Successfully",
                        icon: "success",
                        confirmButtonClass: "btn btn-primary w-xs mt-2",
                        buttonsStyling: !1,

                    }).then((res) => {
                        // Code to run when the user clicks the "OK" button
                        if (res.isConfirmed) {
