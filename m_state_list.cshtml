        function set_m_state_id(val) {
            document.getElementById("state_id").value = val;
        }

        function update_m_state_id(id) {

            $.ajax({
                //type: "GET",
                url: '@Url.Action("edit_m_state", "Master")',
                data: { id: id },
            }).done(function (res) {

                if (res.value == "True") {

                    document.getElementById("state_id_up").value = res.state_id;
                    document.getElementById("country_id_up").value = res.country_list.country_id;

                    document.getElementById("state_name_up").value = res.state_name;

                }
                if (res.value == "False") {
                    alert("Value can not be null");
                }

            });

        }


        $("#search_items_filter").change(function () {
            var selected = document.getElementById("search_items_filter").value;
            if (selected == "all") {
                document.getElementById("search_item_text").required = false;
            }
            else {
                document.getElementById("search_item_text").required = true;
            }
        });

        $("#item_count").change(function () {

